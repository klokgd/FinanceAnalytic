<Window x:Class="FinanceAnalytic.AddAccount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:FinanceAnalytic"
        mc:Ignorable="d"
        Title="Добавление счета" Height="450" Width="400">
    <Grid>
        <TextBox x:Name="TextBoxAccounName" HorizontalAlignment="Center" materialDesign:HintAssist.Hint="Название счета" Margin="0,129,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="266"/>
        <TextBox x:Name="TextBoxSumAccount" HorizontalAlignment="Center" materialDesign:HintAssist.Hint="Начальная сумма" Margin="0,192,0,0" VerticalAlignment="Top" Width="266"/>
        <Button x:Name="button_AddAccount" Content="Добавить счет" HorizontalAlignment="Center" Margin="0,342,0,0" VerticalAlignment="Top" Click="button_AddAccount_Click"/>
    </Grid>
</Window>
