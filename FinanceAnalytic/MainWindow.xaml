<Window x:Class="FinanceAnalytic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinanceAnalytic"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button_Plus" HorizontalAlignment="Left" Margin="439,122,0,0" FontSize="20" VerticalAlignment="Top" RenderTransformOrigin="-5.745,-6.101" Height="80" Width="282" Click="button_Plus_Click" Content="Плюс к экземпляру счета"/>
        <!--<Frame x:Name="frame" Content="Frame" Navigated="frame_Navigated" Margin="439,122,195,232"/>-->
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" FontSize="24" Margin="104,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="131" Width="265" RenderTransformOrigin="-0.198,-0.306"/>
        <Button x:Name="button_Minus" HorizontalAlignment="Left" Margin="439,262,0,0" FontSize="20" VerticalAlignment="Top" RenderTransformOrigin="-5.745,-6.101" Height="80" Width="325" Click="button_Minus_Click" Content="Минус от экземпляра счета"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" FontSize="24" Margin="100,217,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="173" Height="50" TextChanged="textBox_TextChanged" />
        <Button x:Name="button" Content="Все счета" HorizontalAlignment="Left" Margin="228,328,0,0" VerticalAlignment="Top" Height="57" Width="141" RenderTransformOrigin="0.5,0.5" Click="button_Click">

        </Button>

        <!--<Frame x:Name="frame_Copy" Content="Frame" Navigated="frame_Navigated" Margin="439,122,195,232"/>-->

    </Grid>
</Window>
